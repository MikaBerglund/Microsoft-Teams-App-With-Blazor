@page "/personal"
@code {
    TeamsApplication app;
}
<TeamsApplication @ref="this.app">
    <ApplicationTemplate Context="facade">

        <h3>Personal Page (Server)</h3>

        <ContextViewer Context="facade?.Context"></ContextViewer>

        <LoginButton LoginHint="@facade.Context.LoginHint">Log in as @facade.Context.LoginHint</LoginButton>
    </ApplicationTemplate>
</TeamsApplication>
